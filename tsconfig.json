{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "isolatedModules": false,
    "paths": {
      "buffer": [
        "node_modules/buffer"
      ]
    }
  },
  "exclude": [
    "./dist",
